#include <tunables/global>

/usr/bin/apt-get {
  # filesystem + libs
  #include <abstractions/base>

  # network for fetching packages
  network inet stream,
  network inet6 stream,

  # config + lists
  /etc/apt/** r,
  /var/lib/apt/lists/** r,
  /var/cache/apt/archives/** rw,

  # the binary
  /usr/bin/apt-get mr,
}
